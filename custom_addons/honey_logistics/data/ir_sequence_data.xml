<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_honey_shipment" model="ir.sequence">
        <field name="name">Honey Shipment</field>
        <field name="code">honey.shipment</field>
        <field name="prefix">SHIP</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
    </record>

    <record id="seq_honey_qr_confirmation" model="ir.sequence">
        <field name="name">Honey QR Confirmation</field>
        <field name="code">honey.qr.confirmation</field>
        <field name="prefix">QR</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
    </record>

    <record id="seq_honey_return_request" model="ir.sequence">
        <field name="name">Honey Return Request</field>
        <field name="code">honey.return.request</field>
        <field name="prefix">RET</field>
        <field name="padding">6</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
    </record>
</odoo>
